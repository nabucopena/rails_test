<p style="color: green"><%= notice %></p>

<h1>Your Projects</h1>

<div id="projects">
  <% @projects.each do |project| %>
    <%= render project %>
    <p>
      <%= link_to "Show", project %>
      <%= link_to "Edit", edit_project_path(project) %>
    </p>
  <% end %>
</div>

<%= link_to "New project", new_project_path %>
